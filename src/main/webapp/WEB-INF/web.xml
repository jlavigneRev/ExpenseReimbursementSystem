<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>loginValidation</servlet-name>
    <servlet-class>com.revature.servlets.LoginValidationServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>about</servlet-name>
    <servlet-class>com.revature.servlets.AboutServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>employeeLanding</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeLandingServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>employeeSubmitRequest</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeSubmitReqServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>employeeReqProcessing</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeReqProcessingServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>employeeViewRequest</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeViewReqServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>managerLanding</servlet-name>
    <servlet-class>com.revature.servlets.ManagerLandingServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>managerViewRequest</servlet-name>
    <servlet-class>com.revature.servlets.ManagerViewReqServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>loginValidation</servlet-name>
    <url-pattern>/loginValidation</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>about</servlet-name>
    <url-pattern>/about</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>employeeLanding</servlet-name>
    <url-pattern>/employeeLanding</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>employeeSubmitRequest</servlet-name>
    <url-pattern>/employeeSubmitRequest</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>employeeReqProcessing</servlet-name>
    <url-pattern>/employeeReqProcessing</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>employeeViewRequest</servlet-name>
    <url-pattern>/employeeViewRequest</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>managerLanding</servlet-name>
    <url-pattern>/managerLanding</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>managerViewRequest</servlet-name>
    <url-pattern>/managerViewRequest</url-pattern>
  </servlet-mapping>
</web-app>
